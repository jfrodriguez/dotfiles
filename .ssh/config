Host *
    Protocol 2
    Compression yes
    PreferredAuthentications publickey,password
    GSSAPIAuthentication no
    ServerAliveInterval 30

Host BASTION_HOST
   ForwardAgent yes
   User DEFAULT_USER

Host PRIVATE_SERVER
   ProxyCommand ssh -C BASTION_HOST nc %h %p
   User USERNAME



PROXY SOCKS
ssh -C2TvnN -D 1080 BASTION_HOST
